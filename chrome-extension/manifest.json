{
    "manifest_version": 2,
    "name": "Medium Proxy",
    "version": "0.0.1",
    "description": "Proxy medium.com",
    "icons": {
        "16": "16x16.png",
        "48": "48x48.png",
        "128": "128x128.png"
    },
    "browser_action": {
        "default_title": "MediumProxy",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.googleapis.com https://*.bootstrapcdn.com https://code.jquery.com; object-src 'self'",
    "offline_enabled": true,
    "permissions": [
        "<all_urls>",
        "webRequest",
        "webRequestBlocking"
    ]
}
